{
  "name": "wackanel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow bun",
    "dev": "next dev --turbopack",
    "build": "bun run db:migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "db:generate": "bun run db:generate:auth && bun run db:generate:migrations",
    "db:generate:auth": "bunx @better-auth/cli@latest generate --config ./src/lib/auth/index.ts --output ./src/lib/database/drizzle/schema/auth.ts -y",
    "db:generate:migrations": "drizzle-kit generate",
    "db:migrate": "bun run src/lib/database/drizzle/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.1.0",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lru-cache": "^7.10.10",
    "better-auth": "^1.2.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "easy-peasy": "^6.1.0",
    "framer-motion": "^12.18.1",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "jose": "^6.0.11",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.516.0",
    "nanoid": "^5.1.5",
    "next": "^15.4.4",
    "postgres": "^3.4.7",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bun": "^1.2.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "pg": "^8.16.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
